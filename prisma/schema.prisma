generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  email    String @unique
  password String
  todos    Todo[]
}

model Todo {
  id     Int    @id @default(autoincrement())
  task   String
  userId Int
  user   User   @relation(fields: [userId], references: [id])
}
